<?xml version='1.0' encoding='UTF-8'?>
<model xmlns="http://www.cellml.org/cellml/1.0#" xmlns:cellml="http://www.cellml.org/cellml/1.0#" xmlns:cmeta="http://www.cellml.org/metadata/1.0#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" name="compensated_voltage_clamp_mc">
  <units name="A_per_F">
    <unit units="gram" multiplier="1000"/>
    <unit units="metre" exponent="2.0"/>
    <unit units="second" exponent="-4.0"/>
    <unit units="ampere" exponent="-1.0"/>
  </units>
  <units name="Ohm_times_1e12">
    <unit units="gram" multiplier="1000000000000"/>
    <unit units="metre" exponent="2.0"/>
    <unit units="second" exponent="-3.0"/>
    <unit units="ampere" exponent="-2.0"/>
  </units>
  <units name="mV">
    <unit units="gram"/>
    <unit units="metre" exponent="2.0"/>
    <unit units="second" exponent="-3.0"/>
    <unit units="ampere" exponent="-1.0"/>
  </units>
  <units name="ms">
    <unit units="second" multiplier="0.001"/>
  </units>
  <units name="nS">
    <unit units="gram" exponent="-1.0" multiplier="1e-12"/>
    <unit units="metre" exponent="-2.0"/>
    <unit units="second" exponent="3.0"/>
    <unit units="ampere" exponent="2.0"/>
  </units>
  <units name="pA">
    <unit units="ampere" multiplier="1e-12"/>
  </units>
  <units name="pF">
    <unit units="gram" exponent="-1.0" multiplier="1e-15"/>
    <unit units="metre" exponent="-2.0"/>
    <unit units="second" exponent="4.0"/>
    <unit units="ampere" exponent="2.0"/>
  </units>
  <component name="engine">
    <variable name="pace" units="dimensionless" public_interface="out" initial_value="0.0"/>
    <variable name="time" units="ms" public_interface="out"/>
  </component>
  <component name="mc">
    <variable name="I" units="pA" public_interface="out"/>
    <variable name="Icc" units="pA" initial_value="0.0"/>
    <variable name="V" units="mV" public_interface="in"/>
    <variable name="cprs" units="pF" public_interface="in"/>
    <variable name="dVdt" units="A_per_F" public_interface="in"/>
    <variable name="g" units="nS"/>
    <variable name="rseries" units="Ohm_times_1e12" public_interface="in"/>
    <variable name="time" units="ms" public_interface="in"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>I</ci>
        <apply>
          <plus/>
          <apply>
            <times/>
            <ci>g</ci>
            <ci>V</ci>
          </apply>
          <ci>Icc</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>Icc</ci>
        </apply>
        <apply>
          <times/>
          <ci>rseries</ci>
          <apply>
            <minus/>
            <ci>dVdt</ci>
            <apply>
              <divide/>
              <ci>Icc</ci>
              <ci>cprs</ci>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>g</ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless">0.1524</cn>
          <cn cellml:units="nS">1000.0</cn>
        </apply>
      </apply>
    </math>
  </component>
  <component name="membrane">
    <variable name="I" units="pA" public_interface="in"/>
    <variable name="V" units="mV" public_interface="out" initial_value="0.0"/>
    <variable name="Vp" units="mV" public_interface="in"/>
    <variable name="cm" units="pF" initial_value="100.0"/>
    <variable name="dVdt" units="A_per_F" public_interface="out"/>
    <variable name="membrane_dVdt_tau" units="ms"/>
    <variable name="rseries" units="Ohm_times_1e12" public_interface="in"/>
    <variable name="time" units="ms" public_interface="in"/>
    <variable name="voffset_eff" units="mV" public_interface="in"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>V</ci>
        </apply>
        <ci>dVdt</ci>
      </apply>
      <apply>
        <eq/>
        <ci>dVdt</ci>
        <apply>
          <minus/>
          <apply>
            <divide/>
            <apply>
              <plus/>
              <apply>
                <minus/>
                <ci>Vp</ci>
                <ci>V</ci>
              </apply>
              <ci>voffset_eff</ci>
            </apply>
            <ci>membrane_dVdt_tau</ci>
          </apply>
          <apply>
            <divide/>
            <ci>I</ci>
            <ci>cm</ci>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>membrane_dVdt_tau</ci>
        <apply>
          <times/>
          <ci>cm</ci>
          <ci>rseries</ci>
        </apply>
      </apply>
    </math>
  </component>
  <component name="voltageclamp">
    <variable name="Iin" units="pA"/>
    <variable name="Iout" units="pA" initial_value="0.0"/>
    <variable name="V" units="mV" public_interface="in"/>
    <variable name="Vc" units="mV"/>
    <variable name="Vclamp" units="mV" initial_value="0.0"/>
    <variable name="Vest" units="mV" initial_value="0.0"/>
    <variable name="Vp" units="mV" public_interface="out" initial_value="0.0"/>
    <variable name="alpha" units="dimensionless" initial_value="0.8"/>
    <variable name="cm_est" units="pF"/>
    <variable name="cprs" units="pF" public_interface="out" initial_value="4.0"/>
    <variable name="cprs_est" units="pF"/>
    <variable name="dVclampdt" units="A_per_F"/>
    <variable name="dVestdt" units="A_per_F"/>
    <variable name="dVpdt" units="A_per_F"/>
    <variable name="pace" units="dimensionless" public_interface="in"/>
    <variable name="rseries" units="Ohm_times_1e12" public_interface="out"/>
    <variable name="rseries_est" units="Ohm_times_1e12"/>
    <variable name="tau_clamp" units="ms"/>
    <variable name="tau_sum" units="ms" initial_value="0.04"/>
    <variable name="time" units="ms" public_interface="in"/>
    <variable name="voffset_eff" units="mV" public_interface="out" initial_value="5.0"/>
    <variable name="voltageclamp_Iout_tau" units="ms"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>Iin</ci>
        <apply>
          <minus/>
          <apply>
            <minus/>
            <apply>
              <plus/>
              <apply>
                <divide/>
                <apply>
                  <plus/>
                  <apply>
                    <minus/>
                    <ci>Vp</ci>
                    <ci>V</ci>
                  </apply>
                  <ci>voffset_eff</ci>
                </apply>
                <ci>rseries</ci>
              </apply>
              <apply>
                <times/>
                <ci>cprs</ci>
                <ci>dVpdt</ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci>cprs_est</ci>
              <ci>dVclampdt</ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci>cm_est</ci>
            <piecewise>
              <piece>
                <ci>dVclampdt</ci>
                <apply>
                  <lt/>
                  <ci>rseries_est</ci>
                  <cn cellml:units="dimensionless">1e-06</cn>
                </apply>
              </piece>
              <otherwise>
                <ci>dVestdt</ci>
              </otherwise>
            </piecewise>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>Iout</ci>
        </apply>
        <apply>
          <divide/>
          <apply>
            <minus/>
            <ci>Iin</ci>
            <ci>Iout</ci>
          </apply>
          <ci>voltageclamp_Iout_tau</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>Vc</ci>
        <ci>pace</ci>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>Vclamp</ci>
        </apply>
        <ci>dVclampdt</ci>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>Vest</ci>
        </apply>
        <ci>dVestdt</ci>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>Vp</ci>
        </apply>
        <ci>dVpdt</ci>
      </apply>
      <apply>
        <eq/>
        <ci>cm_est</ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless">0.9</cn>
          <cn cellml:units="pF">100.0</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>cprs_est</ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless">4.0</cn>
          <cn cellml:units="pF">0.9</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>dVclampdt</ci>
        <apply>
          <divide/>
          <apply>
            <minus/>
            <apply>
              <plus/>
              <ci>Vc</ci>
              <apply>
                <times/>
                <apply>
                  <plus/>
                  <ci>Iout</ci>
                  <apply>
                    <times/>
                    <ci>cm_est</ci>
                    <ci>dVestdt</ci>
                  </apply>
                </apply>
                <ci>rseries_est</ci>
              </apply>
            </apply>
            <ci>Vclamp</ci>
          </apply>
          <ci>tau_sum</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>dVestdt</ci>
        <piecewise>
          <piece>
            <cn cellml:units="dimensionless">0.0</cn>
            <apply>
              <lt/>
              <ci>cm_est</ci>
              <cn cellml:units="dimensionless">1e-06</cn>
            </apply>
          </piece>
          <piece>
            <cn cellml:units="dimensionless">0.0</cn>
            <apply>
              <lt/>
              <ci>rseries_est</ci>
              <cn cellml:units="dimensionless">1e-06</cn>
            </apply>
          </piece>
          <otherwise>
            <apply>
              <divide/>
              <apply>
                <minus/>
                <ci>Vc</ci>
                <ci>Vest</ci>
              </apply>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <apply>
                    <times/>
                    <apply>
                      <minus/>
                      <cn cellml:units="dimensionless">1.0</cn>
                      <ci>alpha</ci>
                    </apply>
                    <ci>cm_est</ci>
                  </apply>
                  <ci>rseries_est</ci>
                </apply>
                <ci>alpha</ci>
              </apply>
            </apply>
          </otherwise>
        </piecewise>
      </apply>
      <apply>
        <eq/>
        <ci>dVpdt</ci>
        <apply>
          <divide/>
          <apply>
            <minus/>
            <ci>Vclamp</ci>
            <ci>Vp</ci>
          </apply>
          <ci>tau_clamp</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>rseries</ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless">10.0</cn>
          <cn cellml:units="Ohm_times_1e12">0.001</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>rseries_est</ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless">0.9</cn>
          <cn cellml:units="Ohm_times_1e12">0.01</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>tau_clamp</ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless">0.8</cn>
          <cn cellml:units="dimensionless">0.001</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>voltageclamp_Iout_tau</ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless">7.5</cn>
          <cn cellml:units="dimensionless">0.001</cn>
        </apply>
      </apply>
    </math>
  </component>
  <connection>
    <map_components component_1="engine" component_2="mc"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  <connection>
    <map_components component_1="engine" component_2="membrane"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  <connection>
    <map_components component_1="engine" component_2="voltageclamp"/>
    <map_variables variable_1="pace" variable_2="pace"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  <connection>
    <map_components component_1="mc" component_2="membrane"/>
    <map_variables variable_1="I" variable_2="I"/>
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="dVdt" variable_2="dVdt"/>
  </connection>
  <connection>
    <map_components component_1="mc" component_2="voltageclamp"/>
    <map_variables variable_1="cprs" variable_2="cprs"/>
    <map_variables variable_1="rseries" variable_2="rseries"/>
  </connection>
  <connection>
    <map_components component_1="membrane" component_2="voltageclamp"/>
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="Vp" variable_2="Vp"/>
    <map_variables variable_1="rseries" variable_2="rseries"/>
    <map_variables variable_1="voffset_eff" variable_2="voffset_eff"/>
  </connection>
</model>
